{
  "name": "adp-nodejs-chassis-library",
  "version": "0.1.0",
  "description": "Library for common utils/services to be used in EEA node applications",
  "engines": {
    "node": "20.12.2"
  },
  "type": "module",
  "types": "bundle.d.ts",
  "scripts": {
    "lint:js": "eslint \"**/*.js\"",
    "lint:markdown": "markdownlint ./**/*.md",
    "lint:package-lock:pkg-auth": "lockfile-lint -p ./packages/auth/package-lock.json",
    "lint:package-lock:pkg-base": "lockfile-lint -p ./packages/base/package-lock.json",
    "lint:package-lock:pkg-database-pg": "lockfile-lint -p ./packages/database-pg/package-lock.json",
    "lint:package-lock:pkg-developer-tools": "lockfile-lint -p ./packages/developer-tools/generator-npm-library/package-lock.json",
    "lint:package-lock:pkg-faultHandler": "lockfile-lint -p ./packages/faultHandler/package-lock.json",
    "lint:package-lock:pkg-kubernetes": "lockfile-lint -p ./packages/kubernetes/package-lock.json",
    "lint:package-lock:pkg-license-manager": "lockfile-lint -p ./packages/license-manager/package-lock.json",
    "lint:package-lock:pkg-pm-service": "lockfile-lint -p ./packages/pm-service/package-lock.json",
    "lint:package-lock:pkg-telemetry": "lockfile-lint -p ./packages/telemetry/package-lock.json",
    "lint:package-lock:pkg-ui-common": "lockfile-lint -p ./packages/ui-common/package-lock.json",
    "lint:package-lock:pkg-ui-components": "lockfile-lint -p ./packages/ui-components/package-lock.json",
    "lint:package-lock:pkg-utilities": "lockfile-lint -p ./packages/utilities/package-lock.json",
    "lint:package-lock:pkg-ui-settings": "lockfile-lint -p ./packages/ui-settings/package-lock.json",
    "lint:package-lock:all": "lockfile-lint -p package-lock.json && concurrently 'npm:lint:package-lock:pkg-*'",
    "build": "echo \"Build script placeholder \" && exit 0 ",
    "generate-api-docs": "node scripts/generateApiDocs.js",
    "update-dev-dependencies": "npm-check-updates --deep --dep dev --mergeConfig -u",
    "checkjs": "tsc",
    "clean": "rm -rf node_modules && rm -rf packages/*/node_modules",
    "checkjs:all": "npm run checkjs && scripts/run-npm-all.sh 'npm run checkjs'",
    "build:all": "scripts/run-npm-all.sh 'npm run build'",
    "test:all": "scripts/run-npm-all.sh 'npm run test'",
    "test:full:all": "scripts/run-npm-all.sh 'npm run test:full:timeout'",
    "safePublish:dryRun:all": "scripts/run-npm-all.sh 'npm run safePublish dry-run'",
    "safePublish:all": "scripts/run-npm-all.sh 'npm run safePublish'",
    "ci:all": "npm ci && scripts/run-npm-all.sh 'npm ci'",
    "install:all": "npm i && scripts/run-npm-all.sh 'npm i'",
    "sonar-scanner": "sonar-scanner",
    "outdated:all:generateReport": "node scripts/generate-outdated-3pps-config.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gerrit-gamma.gic.ericsson.se:29418/EEA/adp-nodejs-chassis-library"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@open-wc/eslint-config": "^12.0.3",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-lit": "^1.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.25.1",
    "extract-json-from-string": "^1.0.1",
    "js-yaml": "^4.1.0",
    "jsdoc-to-markdown": "^8.0.1",
    "lockfile-lint": "^4.13.2",
    "markdownlint-cli": "^0.39.0",
    "mocha": "^10.4.0",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^16.14.18",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "sonarqube-scanner": "^3.4.0",
    "testdouble": "^3.20.2",
    "typescript": "^5.4.5",
    "xpath-html": "^1.0.3"
  }
}
